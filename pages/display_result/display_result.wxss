.title {
    font-size: 16pt;
    color: rgb(0, 0, 1);
    margin: 10px;
}
table {
  border: 0px solid darkgray;
 }
 .tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
 }
 .td {
  width:40%;
  justify-content: center;
  text-align: center;
  font-size: 12px;
 }
 .bg-w{
  background: snow;
 }
 .bg-g{
  background: #E6F3F9;
 }
 .bg-e{
  background: #c7ccce;
 }
 .th {
  width: 40%;
  justify-content: center;
  background: #E1B368;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
  font-size: 13px;
 }